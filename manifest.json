{
    "manifest_version" : 2,

    "name" : "Alphabetab",
    "description" : "Sort and kill tabs.",
    "version" : "0.1.0",

    "browser_action" : {
        "default_title" : "Alphabetab",
        "default_icon" : "icon.png",
        "default_popup" : "alphabetab.html"
    },
    "permissions" : [
        "tabs"
    ]
}